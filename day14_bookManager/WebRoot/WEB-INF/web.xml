  </servlet>
  <servlet>
    <servlet-name>GetBookinfoServlet</servlet-name>
    <servlet-class>com.bookservlet.GetBookinfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchAJAXInfoServlet</servlet-name>
    <servlet-class>com.bookservlet.SearchAJAXInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCardServlet</servlet-name>
    <servlet-class>com.bookservlet.AddCardServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ChangeCountServlet</servlet-name>
    <servlet-class>com.bookservlet.ChangeCountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>text</servlet-name>
    <servlet-class>com.bookservlet.text</servlet-class>
  </servlet>

















  <servlet-mapping>
    <servlet-name>LookServlet</servlet-name>
    <url-pattern>/lookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddBookServlet</servlet-name>
    <url-pattern>/addBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindBookByIdServlet</servlet-name>
    <url-pattern>/findBookByIdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateBookServlet</servlet-name>
    <url-pattern>/updateBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteBookServlet</servlet-name>
    <url-pattern>/deleteBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteAllServlet</servlet-name>
    <url-pattern>/deleteAllServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindProductByManyCondition</servlet-name>
    <url-pattern>/findProductByManyCondition</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/pageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetBookinfoServlet</servlet-name>
    <url-pattern>/getBookinfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchAJAXInfoServlet</servlet-name>
